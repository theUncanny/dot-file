{
  "app/*.php": {
    "type": "model"
  },
  "resources/views/*.blade.php": {
    "type": "view"
  },
  "app/Http/Controllers/*Controller.php": {
    "type": "controller"
  },
  "app/Http/routes.php": {
    "type": "initializer"
  },
  "app/Http/Requests/*.php": {
    "type": "request"
  },
  "app/Http/Middleware/*.php": {
    "type": "middleware"
  }
}
